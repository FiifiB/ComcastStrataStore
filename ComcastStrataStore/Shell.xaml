<Window x:Class="ComcastStrataStore.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComcastStrataStore"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:region="clr-namespace:ComcastStrataStore.Infrastructure;assembly=ComcastStrataStore.Infrastructure"
        mc:Ignorable="d"
        Title="Comcast Strata Store" Height="768" Width="1024">
    <DockPanel>
        <ContentControl DockPanel.Dock="Top" Height="Auto" Width="Auto"   prism:RegionManager.RegionName="{x:Static region:RegionNames.HeaderRegion}">
        </ContentControl>
        <ContentControl DockPanel.Dock="Top" Height="Auto" Width="Auto"   prism:RegionManager.RegionName="{x:Static region:RegionNames.MenuBarRegion}">
        </ContentControl>
        <ContentControl  DockPanel.Dock="Top" Height="Auto" Width="Auto"  prism:RegionManager.RegionName="{x:Static region:RegionNames.MainRegion}">
        </ContentControl>
    </DockPanel>
</Window>
